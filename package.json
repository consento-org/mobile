{
  "name": "consento-mobile",
  "description": "Consento Mobile App",
  "scripts": {
    "postinstall": "jetify",
    "android": "expo android",
    "ios": "expo ios",
    "web": "expo start --web",
    "start": "expo start",
    "screenshot": "./screenshot/screenshot-recorder.js",
    "test": "jest",
    "lint": "eslint --ext .js,.ts,.jsx,.tsx .",
    "android:reload": "adb shell input text \"RR\"",
    "android:menu": "adb shell input keyevent 82"
  },
  "dependencies": {
    "@consento/api": "0.0.21",
    "@consento/crypto": "0.0.12",
    "@consento/notification-server": "0.0.18",
    "@consento/shamirs-secret-sharing": "^1.0.4",
    "@types/lodash.throttle": "^4.1.6",
    "@use-it/interval": "^0.1.3",
    "acorn": "^7.1.0",
    "expo": "^36.0.2",
    "expo-camera": "~8.0.0",
    "expo-file-system": "^8.0.0",
    "expo-media-library": "^8.0.0",
    "expo-permissions": "~8.0.0",
    "expo-secure-store": "^8.0.0",
    "expo-sharing": "^8.0.0",
    "fast-equals": "^2.0.0",
    "fast-json-patch": "^3.0.0-1",
    "fbemitter": "^2.1.1",
    "is-url-superb": "^3.0.0",
